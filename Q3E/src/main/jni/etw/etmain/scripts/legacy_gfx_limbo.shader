//========================================//
// Limbo class icons
// Also used for fireteam class icons
// and as voice chat sprites
//========================================//
gfx/limbo/ic_soldier
{
	nocompress
	nopicmip
	{
		map gfx/limbo/ic_soldier.tga
		blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
		rgbGen vertex
	}
}

